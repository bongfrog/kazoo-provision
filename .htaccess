IndexIgnore *
Options +FollowSymLinks
RewriteEngine On
#RewriteCond %{REQUEST_URI} !m3/.* [NC] # Don't rewrite if we already have
#RewriteCond %{REQUEST_URI} !prov/.* [NC] # Don't rewrite if we already have
#RewriteCond %{REQUEST_URI} !settings\.php [NC] # Don't rewrite if we already have
#RewriteCond %{REQUEST_URI} !monster\.php [NC] # Don't rewrite if we already have
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ settings.php [L]

<Files config.json>
    Order allow,deny
    Deny from all
    Allow from 127.0.0.1
</Files>